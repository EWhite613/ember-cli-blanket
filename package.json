{
  "name": "ember-cli-blanket",
  "version": "0.6.0",
  "description": "Blanket code coverage for ember-cli",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "node tests/runner",
    "autotest": "mocha --watch --reporter spec tests/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sglanzer/ember-cli-blanket.git"
  },
  "keywords": [
    "ember",
    "ember-cli",
    "ember-addon",
    "code coverage"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Steven Glanzer <sglanzer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sglanzer/ember-cli-blanket/issues"
  },
  "homepage": "https://github.com/sglanzer/ember-cli-blanket",
  "devDependencies": {
    "broccoli-asset-rev": "^2.1.2",
    "chai": "^2.2.0",
    "ember-cli": "1.13.8",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.1.0",
    "ember-cli-htmlbars": "^1.0.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.0.1",
    "ember-cli-release": "0.2.3",
    "ember-export-application-global": "^1.0.4",
    "ember-resolver": "^2.0.2",
    "glob": "^5.0.3",
    "mocha": "^2.2.1",
    "tmp-sync": "^1.0.1"
  },
  "dependencies": {
    "body-parser": "^1.12.2",
    "broccoli-funnel": "^0.2.2",
    "broccoli-merge-trees": "^0.2.1",
    "fs-extra": "^0.18.0",
    "core-object": "^1.1.0",
    "lodash": "^3.6.0"
  },
  "ember-addon": {
    "after": "ember-cli-qunit",
    "before": "proxy-server-middleware",
    "configPath": "tests/dummy/config"
  }
}
